MDBoxLayout: 
    orientation: 'vertical'

    MDToolbar:
        title: 'PomoDuno'
        left_action_items: [['menu']]
        right_action_items: [['dots-vertical']]       

    MDFloatLayout:  
        MDIconButton:  
            icon: 'timer'
            user_font_size: 100
            pos_hint: {'center_x': .5,'center_y': .8}
        MDLabel:
            text: '25:00'
            font_style: 'H2'
            halign: 'center'
        
        MDRaisedButton:
            text: 'Iniciar'
            size_hint_x: 0.8
            pos_hint: {'center_x': .5,'center_y': 1/3}

